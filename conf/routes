# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                                       Application.index
GET     /alta.html                              Application.alta
GET     /altarazas.html                         Application.altarazas
GET     /cambio.html                            Application.cambio
GET     /cambiorazas.html                       Application.cambiorazas
GET     /baja.html                              Application.baja
GET     /bajarazas.html                         Application.bajarazas

# Ignore favicon requests
GET     /favicon.ico                            404

# Map static resources from the /app/public folder to the /public path
GET     /public/                                staticDir:public

# Catch all
*       /{controller}/{action}                  {controller}.{action}
